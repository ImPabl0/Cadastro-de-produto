<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="272.0" prefWidth="518.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.pablo.cadastropoduto.CadastroController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="lblNome" strokeType="OUTSIDE" strokeWidth="0.0" text="Cadastro">
      <font>
         <Font size="32.0" />
      </font>
   </Text>
   <VBox prefHeight="243.0" prefWidth="478.0" spacing="5.0">
      <children>
         <Label text="Nome" />
         <TextField fx:id="txtNome" />
         <Label text="Quantidade" />
         <TextField fx:id="txtQuantidade" />
         <Button fx:id="btnSalvar" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#salvarProduto" text="Salvar">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Button>
      </children>
   </VBox>
</VBox>
